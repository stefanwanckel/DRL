<mujoco>
    <asset>
        <texture type="skybox" builtin="gradient" rgb1="0.44 0.85 0.56" rgb2="0.46 0.87 0.58" width="32" height="32"></texture>
        <texture name="texture_block" file="block.png" gridsize="3 4" gridlayout=".U..LFRB.D.."></texture>
        <material name="floor_mat" specular="0" shininess="0.5" reflectance="0" rgba="0.2 0.2 0.2 1"></material>
        <material name="table_mat" specular="1" shininess="0.5" reflectance="1" emission="0.1" rgba="0.7 0.7 0.7 1"></material>
        <material name="block_mat" specular="0" shininess="0.5" reflectance="0" rgba="0.2 0.2 0.2 1"></material>
        <material name="puck_mat" specular="0" shininess="0.5" reflectance="0" rgba="0.2 0.2 0.2 1"></material>
        <material name="robot0:base_mat" shininess="0.03" specular="0.4" reflectance="0"></material>
        <material name="robot0:mat0" specular="1" shininess="0.5" reflectance="1" emission="0.25" rgba="0.82 0.82 0.82 1"></material>
        <material name="robot0:mat1" specular="1" shininess="0.5" reflectance="1" emission="0" rgba="0.678 0.847 0.902 1"></material>
        
        
        <!--Meshes-->
        <mesh name="robot0:base"           file="base.stl"     />
        <mesh name="robot0:shoulder"       file="shoulder.stl" />
        <mesh name="robot0:upperarm"       file="upperarm.stl" />
        <mesh name="robot0:forearm"        file="forearm.stl"  />
        <mesh name="robot0:wrist1"         file="wrist1.stl"   />
        <mesh name="robot0:wrist2"         file="wrist2.stl"   />
        <mesh name="robot0:wrist3"         file="wrist3.stl"   />
        <!-- <mesh name="ee_mount"       file="end_effector_mount.STL"   scale="0.001 0.001 0.001"/> -->
    </asset>

    <equality>
        <weld body1="robot0:mocap" body2="dummy_gripper" solimp="0.9 0.95 0.001" solref="0.02 1"></weld>
    </equality>

    <!--Contact config. We dont need contact exemptions for ur5-->
    <contact>
        <!-- for no gripper version we dont need to check if there is contact between gripper fingers -->
        <!-- <exclude body1="robot0:r_gripper_finger_link" body2="robot0:l_gripper_finger_link"></exclude> -->
        <!-- <exclude body1="robot0:torso_lift_link" body2="robot0:torso_fixed_link"></exclude>
        <exclude body1="robot0:Verbesserungentorso_lift_link" body2="robot0:shoulder_pan_link"></exclude> -->
    </contact>
    
    <default>
        <geom contype='1' conaffinity='1' condim='4'  margin="1e-3" solref=".01 1" solimp=".99 .99 .01" />
        <joint armature="0.01" damping=".1"/>
        <default class="UR5">
            <joint damping='65'/>
            <!-- <default class="robot0:Gripper">
                <geom condim="4" margin="0.001" type="box" user="0"></geom>
                <joint armature="100" damping="1000" limited="true" solimplimit="0.99 0.999 0.01" solreflimit="0.01 1" type="slide"></joint>
            </default> -->
            <default class="cls_dummy_gripper">
                <geom condim="4" margin="0.001" type="box" user="0" rgba="0.356 0.361 0.376 1.0"></geom>
                <joint armature="100" damping="1000" limited="true" solimplimit="0.99 0.999 0.01" solreflimit="0.01 1" type="slide"></joint>
            </default>
        </default>

        <default class="UR5e">
            <!-- <joint damping='45'/> -->
        </default>
        <!-- <default class="GRIPPER">
            <joint damping='5'/>
        </default> -->
    </default>
</mujoco>
